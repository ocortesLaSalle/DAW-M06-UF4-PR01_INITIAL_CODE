{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "import/extensions": "off",
    "no-alert": "off",
    "no-restricted-syntax": "off",
    "import/prefer-default-export": "off"
  }
}
